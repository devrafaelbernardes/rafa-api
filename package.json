{
  "name": "rafa-api",
  "version": "1.0.0",
  "description": "Api em NodeJS",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production nodemon --exec babel-node ./server.js",
    "start-minify": "node ./api_server.js",
    "start-dev": "nodemon --exec babel-node ./server.js",
    "start-prod": "npm run build && npm run test-prod",
    "test-prod": "node build/api_server.js",
    "build": "rm -rf build && webpack",
    "migration:run": "knex migrate:latest",
    "migration:revert": "knex migrate:down"
  },
  "repository": {
    "type": "git",
